	build seqword_post;
}
:wseg source0, desc_lim seqword_pre^seqword_post is source0 & desc_lim & (segfld_sel=6 | segfld_sel=0) & (opcode=0xc6b | opcode=0xc7b); seqword_pre & seqword_post {
